{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^react-router-dom$",
    "^react(.*)$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@/store/(.*)$",
    "^@/api/(.*)$",
    "^@/hooks/(.*)$",
    "^@/util(.*)$",
    "^@/utils/(.*)$",
    "^@/util/(.*)$",
    "^@/constants",
    "^@/constants/(.*)$",
    "^@/pages/(.*)$",
    "^@/services/(.*)$",
    "^@/components/(.*)$",
    "^@/assets/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
